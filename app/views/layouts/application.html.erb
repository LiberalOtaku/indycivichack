<!DOCTYPE html>
<html>
<head>
  <title>IndyCivicHack</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="//maps.google.com/maps/api/js?v=3.23&key=<%= ENV['GMAPS_API_KEY'] %>"></script>
  <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
  <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->

</head>
<body>

<%= yield %>
<script>
function initialize() {
  var initLatLng;
  if (navigator.geolocation) {
    navigator.geolocation.getCurrentPosition(function (pos) {
      if (pos) {
        console.log("HEYYY");
      };
      initLatLng = { lat: pos.coords.latitude, lng: pos.coords.longitude };
      console.log("initLatLng: ", initLatLng);

      var mapProp = {
        center:new google.maps.LatLng(initLatLng),
        zoom:15,
        mapTypeId:google.maps.MapTypeId.ROADMAP
      };
      var map=new google.maps.Map(document.getElementById("map"),mapProp);
    });
  } else {
    console.log("Geolocation is not supported by this browser.");
    initLatLng = { lat: 39.768377, lng: -86.158042 };
  }

  
}
google.maps.event.addDomListener(window, 'load', initialize);
</script>
</body>
</html>
